  "version": "0.2.0",
  "configurations": [
    {
      "name": "Edge",
      "type": "pwa-msedge",
      "request": "launch",
      "url": "http://localhost:4200/#",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true,
      "trace": true,
      "userDataDir": "${workspaceRoot}/.vscode/edge"
    },
    {
      "name": "ng test",
      "type": "vscode-edge-devtools.debug",
      "request": "launch",
      "preLaunchTask": "npm: test",
      "url": "http://localhost:9876/debug.html"
    }
  ]
}
